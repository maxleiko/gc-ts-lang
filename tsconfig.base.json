{
  "compilerOptions": {
    "module": "esnext",
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "removeComments": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "jsx": "react",
    "target": "es5",
    "outDir": "./.out",
    "lib": [
      "es2017"
    ]
  }
}
